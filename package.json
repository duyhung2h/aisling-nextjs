{
  "name": "nextjs-shopify",
  "version": "1.0.0",
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "prestart": "nvm use 18.17.0",
    "dev": "next -p 8080",
    "build": "next build",
    "start": "next -p 8080",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/react": "^3.1.1",
    "@builder.io/utils": "1.1.10",
    "@builder.io/widgets": "1.2.24",
    "@emotion/react": "^11.10.6",
    "@mikecousins/react-pdf": "^7.1.0",
    "@netlify/plugin-nextjs": "4.30.4",
    "@radix-ui/react-dialog": "^1.0.2",
    "@reach/dialog": "^0.18.0",
    "@reach/portal": "0.14.0",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf/renderer": "^3.4.4",
    "@swc/helpers": "^0.5.12",
    "@theme-ui/components": "0.15.5",
    "@theme-ui/match-media": "0.15.5",
    "@theme-ui/preset-base": "0.15.5",
    "@theme-ui/presets": "0.15.5",
    "@types/shopify-buy": "2.10.3",
    "@types/traverse": "0.6.32",
    "antd": "^5.22.4",
    "bootstrap": "^5.3.3",
    "cheerio": "1.0.0-rc.12",
    "js-cookie": "3.0.1",
    "next": "^13.5.6",
    "next-seo": "5.15.0",
    "npm": "^10.5.2",
    "pspdfkit": "^2024.5.0",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^9.1.0",
    "react-spring": "^9.6.1",
    "sass": "^1.75.0",
    "shopify-buy": "^2.16.1",
    "stable": "^0.1.8",
    "theme-ui": "0.15.5",
    "traverse": "0.6.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.1.6",
    "@types/js-cookie": "3.0.3",
    "@types/node": "18.17.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "^18.2.25",
    "custom-error": "^0.2.1",
    "encoding": "^0.1.13",
    "jest": "29.4.3",
    "logging-library": "^0.5.0",
    "next-unused": "0.0.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "2.8.4",
    "react-file-viewer": "^0.3.1",
    "typescript": "4.9.5"
  },
  "license": "MIT"
}
